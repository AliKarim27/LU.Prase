<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
    <AssemblyName>LU.Prase.Migrator</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>LU.Prase.Migrator</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>
  <ItemGroup>
    <None Update="appsettings.json;log4net.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Abp.Castle.Log4Net" Version="9.1.0" />
    <PackageReference Include="Abp.HangFire" Version="9.2.2" />
    <PackageReference Include="Abp.HangFire.AspNetCore" Version="9.2.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\LU.Prase.Core\LU.Prase.Core.csproj" />
    <ProjectReference Include="..\LU.Prase.EntityFrameworkCore\LU.Prase.EntityFrameworkCore.csproj" />
  </ItemGroup>
</Project>